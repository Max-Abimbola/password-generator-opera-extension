{
	"manifest_version": 2,

	"name": "Password Generator",
	"description": "An extension to generate a new strong password",
	"version": "1.0",
	"background": {
		"scripts": ["background.js"]
	},

	"permissions": ["tabs"],
	"browser_action": {
		"default_icon": "key-icon.png",
		"default_popup": "popup.html"
		"default_title": "My Sample Extension",

	}
}
